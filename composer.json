{
    "name": "victorhsanjos/padroes-de-projeto",
    "type": "library",
    "description": "Pacote com exemplos de padroes de projetos em PHP.",
    "keywords": ["padroes de projeto"],
    "homepage": "https://github.com/victorhsanjos/padroes-de-projeto",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Hugo",
            "email": "victorhsanjos@gmail.com",
            "homepage": "https://about.me/victoranjos",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Victorhsanjos\\PadroesDeProjeto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Victorhsanjos\\PadroesDeProjeto\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
